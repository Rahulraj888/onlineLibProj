<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
<display-name>online_libraryJavaEE</display-name>

<servlet>
<servlet-name>s1</servlet-name>
<servlet-class>com.library.StudentLogin</servlet-class>
</servlet>

<servlet>
<servlet-name>s2</servlet-name>
<servlet-class>com.library.Subject</servlet-class>
</servlet>

<servlet>
<servlet-name>s3</servlet-name>
<servlet-class>com.library.DisplayBooks</servlet-class>
</servlet>

<servlet>
<servlet-name>s4</servlet-name>
<servlet-class>com.library.DownloadFile</servlet-class>
</servlet>


<servlet-mapping>
<servlet-name>s3</servlet-name>
<url-pattern>/display</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>s4</servlet-name>
<url-pattern>/downlod</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>s1</servlet-name>
<url-pattern>/student</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>s2</servlet-name>
<url-pattern>/subject</url-pattern>
</servlet-mapping>


<welcome-file-list>
<welcome-file>Home.html</welcome-file>
</welcome-file-list>
</web-app>